    //"Jackpot": {status_code: "JPOT", color_code : "#996600"},
    Loading: { status_code: "LDG", color_code: "purple" },
    Unloading: { status_code: "UNLD", color_code: "#191970" },
  };

  $(function () {
    load_data();
    initialize_options_legends();
    get_all_id(); // to set the id's as title
  });

  // to set the id's as title
  function get_all_id() {
    $("#body")
      .find("div")
      .each(function () {
        var titl = this.id.split("_")[1];
        $(this).attr("title", titl);
      });
  }

  /* DRAG TO SCROLL */
  $(function () {
    var curDown = false,
      curYPos = 0,
      curXPos = 0;
    $(window).mousemove(function (m) {
      if (curDown === true) {
        $(window).scrollTop($(window).scrollTop() + (curYPos - m.pageY));
        $(window).scrollLeft($(window).scrollLeft() + (curXPos - m.pageX));
      }
    });

    $(window).mousedown(function (m) {
      curDown = true;
      curYPos = m.pageY;
      curXPos = m.pageX;
    });

    $(window).mouseup(function () {
      curDown = false;
    });
  });

  $(document).on(
    " mouseup mousedown",
    ".header,  .sidenav, #data_container_popup",
    function (e) {
      e.stopPropagation();
    }
  );

  /* DRAG TO SCROLL */

  var header_width;
  $(window).on("load resize ", function () {
    if ($(window).width() < 1200) {
      header_width = 1200;
    } else {
      header_width = $(window).width();
      $(".header").css("left", 0);
    }

    $(".header").width(header_width);
    $(".footer").width(header_width);
  });

  $(window).on("scroll", function (e) {
    if ($(window).width() < 1200) {
      var left = $(this).scrollLeft();
      $(".header").css("left", -left);

      var width = $(".header").width();
      $(".header").width(header_width + left);
    }
  });

  $(".map-menu").on("click", function () {
    var top = $(this).attr("top_loc");
    var left = $(this).attr("left_loc");

    $("html, body").animate({ scrollTop: top - 200 }, "slow");
    $("html, body").animate({ scrollLeft: left - 160 }, "slow");
  });
</script>

<script>
  //Script for zoom feature

  var factor = 1;

  $(".zoom-in").on("change", function (e) {
    e.preventDefault();
    $("#loader").show();

    factor = $(this).val();

    $("#body")
      .children()
      .each(function () {
        //if the element is slot or location or pending request blinker
        if (
          $(this).hasClass("drag-location") ||
          $(this).hasClass("pending-req-indicator") ||
          $(this).hasClass("location_div") ||
          $(this).hasClass("pendingReqDiv")
        ) {
          var e_width = $(this)
            .attr("original-width")
            .replace(/[^-\d\.]/g, "");
          var new_width = e_width * factor;
          var e_height = $(this)
            .attr("original-height")
            .replace(/[^-\d\.]/g, "");
          var new_height = e_height * factor;
          var e_top = $(this)
            .attr("original-top")
            .replace(/[^-\d\.]/g, "");
          var new_top = e_top * factor - (factor * 100 - 100); //For margin fix
          var e_left = $(this)
            .attr("original-left")
            .replace(/[^-\d\.]/g, "");
          var new_left = e_left * factor - (factor * 10 - 10); //For margin fix

          $(this).css("width", new_width + "px");
          $(this).css("height", new_height + "px");
          $(this).css("top", new_top);
          $(this).css("left", new_left);
        }
        //if the element is count for location, it does not have width and height
        else if ($(this).hasClass("location-count")) {
          var e_top = $(this)
            .attr("original-top")
            .replace(/[^-\d\.]/g, "");
          var new_top = e_top * factor - (factor * 100 - 100); //For margin fix
          var e_left = $(this)
            .attr("original-left")
            .replace(/[^-\d\.]/g, "");
          var new_left = e_left * factor - (factor * 10 - 10); //For margin fix

          $(this).css("top", new_top);
          $(this).css("left", new_left);
        }
        $("img").css("zoom", factor);
      });
    $("#loader").hide();
  });

  /*$('.zoom-in').val(1.5);
	$('.zoom-in').trigger("change");*/

  $("#body").bind("mousewheel", function (e) {
    $("#loader").show();
    e.preventDefault();

    if (e.originalEvent.wheelDelta / 120 > 0) {
      factor = factor + 0.1;
      if (factor > 3) {
        factor = 3;
      }
    } else {
      factor = factor - 0.1;
      if (factor < 0.5) {
        factor = 0.5;
      }
    }

    $("#body")
      .children()
      .each(function () {
        //if the element is slot or location or pending request blinker
        if (
          $(this).hasClass("drag-location") ||
          $(this).hasClass("pending-req-indicator") ||
          $(this).hasClass("location_div") ||
          $(this).hasClass("pendingReqDiv")
        ) {
          var e_width = $(this)
            .attr("original-width")
            .replace(/[^-\d\.]/g, "");
          var new_width = e_width * factor;
          var e_height = $(this)
            .attr("original-height")
            .replace(/[^-\d\.]/g, "");
          var new_height = e_height * factor;
          var e_top = $(this)
            .attr("original-top")
            .replace(/[^-\d\.]/g, "");
          var new_top = e_top * factor - (factor * 100 - 100); //For margin fix
          var e_left = $(this)
            .attr("original-left")
            .replace(/[^-\d\.]/g, "");
          var new_left = e_left * factor - (factor * 10 - 10); //For margin fix

          $(this).css("width", new_width + "px");
          $(this).css("height", new_height + "px");
          $(this).css("top", new_top);
          $(this).css("left", new_left);
        }
        //if the element is count for location, it does not have width and height
        else if ($(this).hasClass("location-count")) {
          var e_top = $(this)
            .attr("original-top")
            .replace(/[^-\d\.]/g, "");
          var new_top = e_top * factor - (factor * 100 - 100); //For margin fix
          var e_left = $(this)
            .attr("original-left")
            .replace(/[^-\d\.]/g, "");
          var new_left = e_left * factor - (factor * 10 - 10); //For margin fix

          $(this).css("top", new_top);
          $(this).css("left", new_left);
        }
        $("img").css("zoom", factor);
      });
    $(".zoom-in").val(factor);
    $("#loader").hide();
  });
</script>

